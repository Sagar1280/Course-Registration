<%- include('../partials/header1') %>
<div class="container">
    <div class="main-body">
    
          <!-- Breadcrumb -->
          
          <!-- /Breadcrumb -->
    
          <div class="row gutters-sm">
            <div class="col-md-4 mb-3">
              <div class="card">
                <div class="card-body">
                  <div class="d-flex flex-column align-items-center text-center">
                    <img src="https://static.vecteezy.com/system/resources/previews/000/644/740/original/vector-scientist-or-professor-icon.jpg" alt="Admin" class="rounded-circle" width="150">
                    <div class="mt-3">
                      <h4><%=listItems[0].faculty%></h4>
                      <p class="text-secondary mb-1">NITK</p>
                      <p class="text-muted font-size-sm">Associate professor</p>
                      <!-- <button class="btn btn-primary">Follow</button> -->
                      <button class="btn btn-outline-primary">Message</button>
                    </div>
                  </div>
                </div>
              </div>
              <div class="card mt-3">
                <ul class="list-group list-group-flush">
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><img src="https://tse2.mm.bing.net/th?id=OIP.z9odqgpWjegCidtsWrooqQHaHa&pid=Api&P=0&h=180" height="28" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-globe mr-2 icon-inline"><circle cx="12" cy="12" r="10"></circle><line x1="2" y1="12" x2="22" y2="12" /></line><path d="M12 2a15.3 15.3 0 0 1 4 10 15.3 15.3 0 0 1-4 10 15.3 15.3 0 0 1-4-10 15.3 15.3 0 0 1 4-10z"></path>Registered</h6>
                    <span class="text-secondary">Yes</span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><img src="https://logodix.com/logo/971657.png" height="28" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-globe mr-2 icon-inline"><circle cx="12" cy="12" r="10"></circle><line x1="2" y1="12" x2="22" y2="12" /></line><path d="M12 2a15.3 15.3 0 0 1 4 10 15.3 15.3 0 0 1-4 10 15.3 15.3 0 0 1-4-10 15.3 15.3 0 0 1 4-10z"></path> Credits</h6>
                    <span class="text-secondary">4</span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M9 19c-5 1.5-5-2.5-7-3m14 6v-3.87a3.37 3.37 0 0 0-.94-2.61c3.14-.35 6.44-1.54 6.44-7A5.44 5.44 0 0 0 20 4.77 5.07 5.07 0 0 0 19.91 1S18.73.65 16 2.48a13.38 13.38 0 0 0-7 0C6.27.65 5.09 1 5.09 1A5.07 5.07 0 0 0 5 4.77a5.44 5.44 0 0 0-1.5 3.78c0 5.42 3.3 6.61 6.44 7A3.37 3.37 0 0 0 9 18.13V22"></path>Monday</h6>
                    <span class="text-secondary"><%=listItems1[0].schedule%></span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M23 3a10.9 10.9 0 0 1-3.14 1.53 4.48 4.48 0 0 0-7.86 3v1A10.66 10.66 0 0 1 3 4s-4 9 5 13a11.64 11.64 0 0 1-7 2c9 5 20 0 20-11.5a4.5 4.5 0 0 0-.08-.83A7.72 7.72 0 0 0 23 3z"></path>Tuesday</h6>
                    <span class="text-secondary"><%=listItems1[0].schedule%></span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z"></path><line x1="17.5" y1="6.5" x2="17.51" y2="6.5"></line>Wednesday</h6>
                    <span class="text-secondary"><%=listItems1[0].schedule%></span>
                  </li>
                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z"></path>Thursday</h6>
                    <span class="text-secondary"><%=listItems1[0].schedule%></span>
                  </li>

                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z"></path><h3>Attendance</h3></h6>
                    <span class="text-secondary">

                      <form action="/seeatten" method="POST">
                            
                        <input type="text" class="form-control" name="code" value="<%=listItems1[0].code %>" hidden/>
                        <input type="text" class="form-control" name="id" value="<%=listItems[0].student_id %>" hidden/>

                      <button class="button-68"><img 
                      src="https://www.freeiconspng.com/uploads/search-icon-png-21.png" alt="wrong mark" height="15px"></button>
                    </form>

                    </span>
                  </li>

                  <li class="list-group-item d-flex justify-content-between align-items-center flex-wrap">
                    <h6 class="mb-0"><path d="M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z"></path><h3>Course feed-back</h3></h6>
                    <span class="text-secondary">

                      <form action="/coursefeedback" method="POST">
                            
                        <input type="text" class="form-control" name="code" value="<%=listItems1[0].code %>" hidden/>
                        <input type="text" class="form-control" name="student_id" value="<%=listItems[0].student_id %>" hidden/>
                        <input type="text" class="form-control" name="student" value="<%=listItems[0].student %>" hidden/>
                        <input type="text" class="form-control" name="title" value="<%=listItems[0].title %>" hidden/>
                        <input type="text" class="form-control" name="faculty" value="<%=listItems[0].faculty %>" hidden/>
                        <input type="text" class="form-control" name="faculty_id" value="<%=listItems[0].faculty_id %>" hidden/>

                      <button class="button-68"><img 
                      src="https://tse4.mm.bing.net/th?id=OIP.B8yVPgm_iJ5xYAp55bYWZwHaEr&pid=Api&P=0&h=180" alt="coursefeedback mark" height="30px"></button>
                    </form>

                    </span>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col-md-8">
              <div class="card mb-3">
                <div class="card-body">
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Title</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                        <%=listItems1[0].title%>
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Description</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                        <%=listItems1[0].course_description%>
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Course plan/uploads</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                      
                        <form action="/viewpdf" method="POST">
                          <input type="text" value="<%= listItems1[0].code %>" class="form-control" name="code" hidden>
                          
                    <button class="btn btn-dark rounded-pill px-3" type="submit">View</button>
                        </form>
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Mid-Sem(Out of 50)</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                        <%=listItems[0].midsem_marks%>
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">End-Sem(Out of 100)</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                        <%=listItems[0].endsem_marks%>
                    </div>
                  </div>
                  <hr>
                  <div class="row">
                    <div class="col-sm-3">
                      <h6 class="mb-0">Grade</h6>
                    </div>
                    <div class="col-sm-9 text-secondary">
                        <%=listItems[0].grade%>
                    </div>
                  </div>
                  <hr>
                </div>
              </div>


            </div>
          </div>

        </div>
    </div>

<%- include('../partials/footer') %>